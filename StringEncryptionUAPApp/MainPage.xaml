<Page
    x:Class="StringEncryptionUAPApp.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:StringEncryptionUAPApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Button x:Name="testbutton" Content="Test" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="64" Click="testbutton_Click"/>
        <Button x:Name="exitbutton" Content="Exit" HorizontalAlignment="Stretch" Margin="285,10,10,0" VerticalAlignment="Top" Width="64" Click="exitbutton_Click"/>

    </Grid>
</Page>
